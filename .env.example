# Required: AssemblyAI API Key
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Optional: Environment
ENVIRONMENT=development
DEBUG=false

# Optional: CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8081,https://meetingsai.app,https://www.meetingsai.app

# Webshare Residential Proxy (required for YouTube videos without captions)
# Sign up at https://webshare.io - you need RESIDENTIAL proxies (not free datacenter)
# Format: http://username:password@p.webshare.io:80
WEBSHARE_PROXY_ENABLED=false
WEBSHARE_PROXY_URL=

# Tor proxy (fallback, runs in Docker container)
TOR_PROXY_ENABLED=true
