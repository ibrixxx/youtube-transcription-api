# Required: AssemblyAI API Key
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Optional: Environment
ENVIRONMENT=development
DEBUG=false

# Optional: CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,http://localhost:8081,https://meetingsai.app,https://www.meetingsai.app

# Webshare Rotating Residential Proxy (recommended — highest success rate)
# Sign up at https://proxy2.webshare.io/ — ~$1.40/GB, 80M+ rotating IPs
# Officially recommended by youtube-transcript-api
# 1. Create account → Residential Proxies → Get credentials
# 2. Copy your proxy username and password below
WEBSHARE_PROXY_ENABLED=false
WEBSHARE_PROXY_USERNAME=
WEBSHARE_PROXY_PASSWORD=

# PacketStream Residential Proxy (legacy fallback)
# Sign up at https://packetstream.io - residential proxies at $1/GB
# Format: http://username:password@proxy.packetstream.io:31112
RESIDENTIAL_PROXY_ENABLED=false
RESIDENTIAL_PROXY_URL=

# Tor proxy (fallback, runs in Docker container)
TOR_PROXY_ENABLED=true
